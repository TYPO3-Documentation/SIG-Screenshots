{
	"name": "typo3/cms-base-distribution",
	"description": "TYPO3 CMS Base Distribution",
	"license": "GPL-2.0-or-later",
	"config": {
		"platform": {
			"php": "7.4.1"
		},
		"sort-packages": true
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/susannemoog/TYPO3-Console"
		},
		{
			"type": "vcs",
			"url": "https://github.com/TYPO3-Documentation/introduction"
		},
		{
			"type": "vcs",
			"url": "https://github.com/TYPO3-Documentation/bootstrap_package"
		},
		{
			"type": "vcs",
			"url": "https://github.com/TYPO3-Documentation/styleguide"
		},
		{
			"type": "path",
			"url": "./packages/*"
		}
	],
	"require": {
		"ext-json": "*",
		"bk2k/bootstrap-package": "dev-master",
		"calcinai/php-imagick": "^0.1.2",
		"cweagans/composer-patches": "^1.7",
		"helhum/typo3-console": "latest-dev",
		"t3/screenshots": "@dev",
		"t3docs/examples": "dev-master",
		"t3docs/store-inventory": "^11.0",
		"typo3/cms-backend": "dev-master as 11.1.0",
		"typo3/cms-belog": "dev-master as 11.1.0",
		"typo3/cms-beuser": "dev-master as 11.1.0",
		"typo3/cms-core": "dev-master as 11.1.0",
		"typo3/cms-dashboard": "dev-master as 11.1.0",
		"typo3/cms-extbase": "dev-master as 11.1.0",
		"typo3/cms-extensionmanager": "dev-master as 11.1.0",
		"typo3/cms-felogin": "dev-master as 11.1.0",
		"typo3/cms-filelist": "dev-master as 11.1.0",
		"typo3/cms-fluid": "dev-master as 11.1.0",
		"typo3/cms-fluid-styled-content": "dev-master as 11.1.0",
		"typo3/cms-form": "dev-master as 11.1.0",
		"typo3/cms-frontend": "dev-master as 11.1.0",
		"typo3/cms-impexp": "dev-master as 11.1.0",
		"typo3/cms-info": "dev-master as 11.1.0",
		"typo3/cms-install": "dev-master as 11.1.0",
		"typo3/cms-introduction": "dev-master",
		"typo3/cms-lowlevel": "dev-master as 11.1.0",
		"typo3/cms-recordlist": "dev-master as 11.1.0",
		"typo3/cms-reports": "dev-master as 11.1.0",
		"typo3/cms-rte-ckeditor": "dev-master as 11.1.0",
		"typo3/cms-seo": "dev-master as 11.1.0",
		"typo3/cms-setup": "dev-master as 11.1.0",
		"typo3/cms-styleguide": "dev-master",
		"typo3/cms-sys-note": "dev-master as 11.1.0",
		"typo3/cms-t3editor": "dev-master as 11.1.0",
		"typo3/cms-tstemplate": "dev-master as 11.1.0",
		"typo3/cms-viewpage": "dev-master as 11.1.0"
	},
	"scripts": {
		"typo3-cms-scripts": [
			"typo3cms install:generatepackagestates",
			"typo3cms install:fixfolderstructure"
		],
		"post-autoload-dump": [
			"@typo3-cms-scripts"
		]
	},

	"extra": {
		"patches": {
			"typo3/cms-core": {
				"Dirty: disable form protection, dont use in a public project!!": "patches/DisableFormProtection.patch"
			}
		}
	},
	"require-dev": {
		"bk2k/extension-helper": "^1.0",
		"codeception/codeception": "^4.1",
		"codeception/module-asserts": "^1.0.0",
		"codeception/module-phpbrowser": "^1.0.0"
	}
}
